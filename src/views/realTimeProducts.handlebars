        <input type="number" id="code" name="title" required>

        <label for="Stock">Stock</label>
        <input type="text" id="stock" name="title" required>

        <label for="Category">Categoria</label>
        <input type="text" id="category" name="title" required>

        <button type="submit">Agregar producto</button>
    </form>
</div>
