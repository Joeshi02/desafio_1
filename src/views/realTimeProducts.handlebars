    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>title</th>
                <th>description</th>
                <th>price</th>
                <th>thumbnails</th>
                <th>code</th>
                <th>stock</th>
                <th>category</th>
                <th>status</th>
            </tr>
        </thead>
        <tbody id="body-products">
        </tbody>
    </table>
    <h2>AÃ±adir producto</h2>
    
    <form id="form-products">
        <label for="Title">Titulo</label>
        <input type="text" id="title" name="title" required>

        <label for="Description">Descripcion</label>
        <input type="text" id="description" name="title" required>

        <label for="Price">Precio</label>
        <input type="number" id="price" name="title" required>

